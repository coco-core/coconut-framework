{
  "name": "coconut",
  "version": "0.0.1",
  "description": "构建现代web应用的基于装饰器的javascript框架",
  "main": "./dist/coconut.cjs.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": "./dist/coconut.cjs.js",
    "./jsx-runtime": "./dist/jsx-runtime.cjs.js"
  },
  "scripts": {
    "dev": "rollup -c ./rollup.build.mjs --watch",
    "test": "jest",
    "prettier": "prettier . --write",
    "build:bundle": "rollup -c ./rollup.build.mjs",
    "build:type": "tsc -b ./tsconfig.types.json",
    "build": "pnpm run build:bundle && pnpm run build:type"
  },
  "author": "jiangchenguang",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "rollup": "^4.12.0",
    "tslib": "^2.6.2"
  },
  "jest": {
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    }
  }
}
